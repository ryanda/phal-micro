{
    "name": "ryanda/phal-micro",
    "type": "project",
    "description": "Phalcon Micro-app.",
    "keywords": [
        "phalcon",
        "micro"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}