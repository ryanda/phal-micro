{
    "name": "ryanda/phal-micro",
    "type": "project",
    "description": "Phalcon Micro-app.",
    "keywords": [
        "phalcon",
        "micro"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2 <8.0",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "phalcon/dd": "^1.1"
    },
    "support": {
        "issues": "https://github.com/ryanda/phal-micro/issues",
        "source": "https://github.com/ryanda/phal-micro"
    }
}